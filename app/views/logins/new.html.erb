<h1> This is the login page! </h1>
<%= form_for @user do |f| %>

    <%= f.label :email %><br>
    <h3> <%= f.text_field :email %> </h3>

    <%= f.label :username %><br>
    <h3> <%= f.text_field :username %> </h3>

    <%= f.label :password %><br>
    <h3> <%= f.password_field :password %> </h3>

    <br></br><a href="/logins/login" class="btn btn-default btn-lg">
        <%= f.button "Log In", :controller => 'logins', action: "login" %></a>
    <br></br>

<% end %>